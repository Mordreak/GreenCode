<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" href="data:image/png;base64," type="image/x-icon" />
        <title>{% block title %}Green Dentist  {{ app.request.get('_theme') }}{% endblock %}</title>
        <style>

            *{margin:0px;padding:0px;}
            html {
                height: 100%;
                width: 100%;
            }

            body{
                height: 100%;
                width: 100%;
                {% if app.request.get('_theme') == "bright" %}
                    background-color: #98d17a;
                {% else %}
                    background-color: black;
                {% endif %}
            }

            /* Colors */
            .fbluer {
                color: #62bb49;
            }

            .title-wrapper {
                display: inline-block;
                width: 100%;
                height: auto;
                margin-top: 100px;
            }

            @media all and (max-width: 500px) {
                .title-wrapper {
                    margin-top: 40px;
                }
            }

            .navigation {
                {% if app.request.get('_theme') == "bright" %}
                    background-color:#fff;
                {% else %}
                    background-color: #383838;
                {% endif %}
                width:100%;
                height:50px;
                box-shadow: 0px 1px 1px #c8c8c8;
                line-height:52px;
                position: fixed;
                top: 0;
                z-index: 9999;
                display: flex;
            }

            .fs-32 {
                font-size: 25px;
            }

            .fs-24 {
                font-size:24px;
            }

            .fs-22 {
                font-size:22px;
            }

            .fs-20 {
                font-size: 20px;
            }

            .title {
                margin-right: auto;
                margin-left: 20px;
                font-family: Verdana;
                box-sizing: border-box;
            }

            .title span {
                cursor: pointer;
                display: inline-block;
                width: 100%;
                text-align: center;
                height: 50px;
                box-sizing: border-box;
                float: left;
                {% if app.request.get('_theme') == "bright" %}
                    color: #62bb49;
                {% else %}
                    color: white;
                {% endif %}
            }

            .tagline {
                margin-left: auto;
                margin-right: 20px;
                font-family: sans-serif;
            }

            @media all and (max-width: 500px) {
                .tagline {
                    display: none;
                }
            }

            .fa-home {
                height: 40px;
                padding: 5px;
                margin-left: 10px;
                float: left;
                margin-right: 5px;
            }

            #navigation-title {
                float: left;
                width: auto;
            }

            #home-main-header {
                margin-left: 0;
            }
            #toggle-theme {
                width: 80px;
                height: 80px;
                position: fixed;
                bottom: 40px;
                right: 40px;
                border-radius: 50%;
            }
            #toggle-theme:hover {
                -webkit-box-shadow: 0px 0px 15px 10px rgba(255,255,255,1);
                -moz-box-shadow: 0px 0px 15px 10px rgba(255,255,255,1);
                box-shadow: 0px 0px 15px 10px rgba(255,255,255,1);
            }
            @media all and (max-width: 800px) {
                #toggle-theme {
                    width: 40px;
                    height: 40px;
                    bottom: 20px;
                    right: 20px;
                }
            }
            #toggle-theme img {
                display: inline-block;
                width: 50%;
                margin: 25%;
            }
        </style>
        {% block css %}{% endblock %}
    </head>
    <body>
        {% block nav %}
            <nav class="navigation fbluer">
                <a class="title" href="{{ url('gc_main_homepage', {'_theme': app.request.get('_theme')}) }}">
                   <span class="fs-32">Green Dentist</span>
                </a>
            </nav>
        {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}
            {% if app.request.get('_theme') == 'bright' %}
                {% if 'bright' in app.request.uri %}
                    {% set replacingUri = app.request.uri|replace({'bright': 'dark'}) %}
                {% elseif 'search' in app.request.uri  %}
                    {% set replacingUri = app.request.uri|replace({'search': 'search/dark'}) %}
                {% else %}
                    {% set replacingUri = app.request.uri ~ '/dark' %}
                {% endif %}
                <a style="background-color: white;" href="{{ replacingUri }}" id="toggle-theme"><img alt="toggle-theme-image" src="data:image/png;base64,"/>
                </a>
            {% else %}
                {% set replacingUri = app.request.uri|replace({'dark': 'bright'}) %}
                <a style="background-color: white;" href="{{ replacingUri }}" id="toggle-theme"><img alt="toggle-theme-image" src="data:image/png;base64,"/>
                </a>
            {% endif %}
        {% endblock %}
    </body>
</html>
