{% extends '::base.html.twig' %}
{% block css %}
    {% include '::homepage.html.twig' %}
{% endblock %}
{% block body %}
    <div class="homepage container">
        <div class="title-wrapper">
            <h1><span class="title fs-32">Green Dentist</span></h1>
        </div>
        <form action='{{ url('gc_main_search') }}' class="search-box" id="SBox">
            <input id="search" placeholder="Search..." type="text" name="q" required>
            <button type="submit" class="also search-link" id="searchclick">
                <img
                    src="data:image/svg+xml;base64,"
                    class="search-button"
                />
            </button>
        </form>
    </div>
{% endblock %}
